	Issn = {1000-6087},
	Keywords = {流动人口;社会融合},
	Physicalloc = {CNKI},
	author = {任远 and 邬民乐},
	title = {城市流动人口的社会融合:文献述评},
	journal = {人口研究},
	number = {03},
	pages = {87-94},
	year = {2006},
	tags = "流动人口, 社会融合"
}

