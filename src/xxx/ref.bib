@phdthesis{赵亚男2014,
	Abstract = {农民工定居城市，通过工作和生活条件的改善、社会地位的提升以及与市民的和谐相处实现经济、文化和心理层面的社会融合，这不仅有利于改善农民工自身的生计脆弱性和弱势地位，而且顺应了我国城镇化建设的潮流，对我国和谐社会建设和全面建成小康社会有重要的现实意义。但是，由于农民工受教育程度偏低、接受培训的机会较少，长期从事体力劳动且工作时间长难以保证身体健康，导致其人力资本存量不足；城市生活中与市民的交往较少，而且长期受到市民的歧视，这致使农民工更多依赖血缘、亲缘关系获取实际支持、情感支持和社交支持；农民工工作的不稳定性和权益保障的缺失则加剧了其生计脆弱性，影响了城市生活满意度。目前，从人力资本和社会资本分析农民工社会融合的定性研究较多，定量研究较少，缺乏对社会融合影响因素的系统性解读。本文基于生计脆弱性视角，阐释了人力资本和社会资本的不同指标对社会融合的影响，而且进一步揭示了生活满意度在上述关系中的作用。},
	Keywords = {农民工;人力资本;社会资本;社会融合;生计脆弱性;生活满意度},
	Publisher = {山西师范大学},
	author = {赵亚男},
	title = {农民工人力资本、社会资本与社会融合},
	year = {2014},
	tags = "社会融合, 流动人口"
}

@phdthesis{叶嘉容2012,
	Abstract = {随着我国改革开放的不断深入,农村剩余劳动力不断向城市转移,越来越多的农民进城务工,他们的生存现状引起了学术界和全社会的密切关注。这其中,农民工群体能否顺利的适应城市、融入城市在我国城乡一体化、城市化进程不断加快的背景下更是引起了广泛的讨论。与此同时,农民工群体的社会保障问题一直都是学者关注的焦点,这方面的制度建设和现状令人担忧。在这样的背景下,本文试图把两者有机联系起来,考察两者之间的关系,以期能为农民工的社会保障和社会融合研究提供新的思路和参考。},
	Keywords = {农民工社会保障;社会融合;农民工异质性;Logit模型},
	Publisher = {浙江大学},
	author = {叶嘉容},
