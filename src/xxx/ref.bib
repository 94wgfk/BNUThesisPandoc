	Callnumber = {11-1489/C},
	Issn = {1000-6087},
	Keywords = {流动人口;抽样框;重复抽选及抽选后合并;目标总体与抽样总体的关系;事后加权},
	Physicalloc = {CNKI},
	author = {庄亚儿 and 李伯华},
	title = {流动人口调查抽样的实践与思考},
	journal = {人口研究},
